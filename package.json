{
  "name": "tiny-code-web",
  "private": true,
  "version": "1.0.0",
  "description": "tiny-code-web",
  "keywords": [
    "tiny-code"
  ],
  "author": "jsjzh <kimimi_king@163.com>",
  "license": "MIT",
  "scripts": {
    "start:javascript-handwritten": "npx parcel src/pages/javascript-handwritten/index.html",
    "start:simple-mock": "npx parcel src/pages/simple-mock/index.html",
    "start": "npm run start:simple-mock",
    "format": "prettier --write --cache --parser typescript \"**/*.[tj]s?(x)\"",
    "format:check": "prettier --check --cache --parser typescript \"**/*.[tj]s?(x)\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jsjzh/tiny-code-web.git"
  },
  "bugs": {
    "url": "https://github.com/jsjzh/tiny-code-web/issues"
  },
  "homepage": "https://github.com/jsjzh/tiny-code-web#readme",
  "dependencies": {
    "axios": "^1.4.0",
    "dayjs": "^1.11.9",
    "ramda": "^0.28.0"
  },
  "devDependencies": {
    "@types/node": "^18.17.0",
    "@types/ramda": "^0.28.25",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "buffer": "^5.7.1",
    "eslint": "^8.45.0",
    "eslint-config-alloy": "^4.9.0",
    "parcel": "^2.9.3",
    "typescript": "^4.9.5"
  },
  "alias": {
    "@/*": "./src/$1"
  }
}
